<%= link_to "<< Back to List", time_entries_path  %>

<div>
	<h2>Update Time Entry</h2>
	<%= form_for @time_entry, method: :PUT do |t| %>

	<table summary="fields">
		<tr>
			<th>Name</th>
			<td><%= t.text_field(:name) %></td>
		</tr>
		<tr>
			<th>Start Time</th>
			<td><%= t.text_field(:start_time) %></td>
		</tr>
		<tr>
			<th>End Time</th>
			<td><%= t.text_field(:end_time) %></td>
		</tr>
		<tr>
			<th>Total Time</th>
			<td><%= t.text_field(:total_time) %></td>
		</tr>
	</table>

	<div class="form-buttons">
		<%= submit_tag("Update Time Entry") %>
	</div>
	<% end %>
</div>