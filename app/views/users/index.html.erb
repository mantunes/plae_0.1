
<h3> 
  <%= "Welcome #{@user.first_name}  #{@user.last_name}" %>
</h3>


<div class="container">
  <div class="row">
    <h3>Users</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @users.each do |user| %>
          <tr>
           <td><%= link_to "#{user.first_name}  #{user.last_name}", user %></td>
           <td><%= link_to user.email, user %></td>
         </tr>
         <% end %>
       </tbody>
     </table>
   </div>
 </div>
</div>
